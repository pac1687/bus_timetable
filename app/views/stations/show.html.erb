<div class="page-header">
  <h2><%= @station.name %></h2>
</div>

<h3>Line(s):</h3>
<ul>
  <% @station.lines.each do |line| %>
  <div class="container">
    <li><%= line.name %></li>
  </div>
  <% end %>
</ul>

<a href="/stations/<%= @station.id %>/edit" class="btn btn-warning btn-xs">+ Edit Station</a>
