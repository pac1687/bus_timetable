<div class="form-group">
  <select name="stop[station_id]">
    <% @stations.each do |station|%>
    <option value="<%= station.id %>"> <%= station.name %></option>
    <% end %>
  </select>
</div>
<div class="form-group">
  <select name="stop[line_id]">
    <% @lines.each do |line|%>
    <option value="<%= line.id %>"> <%= line.name %></option>
    <% end %>
  </select>
</div>
